{
  "name": "Chrome Extension Sample",
  "description": "Chrome Extension Sample",
  "version": "0.1",
  "manifest_version": 3,
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icons/logo.png",
    "24": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "permissions": [
    "webRequest",
    "activeTab"
  ],
  "content_scripts": [
    {
      "css": [
        "css/content_css.css"
      ],
      "js": [
        "js/imageAllExchange.js",
        "js/content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": "icons/logo.png",
    "default_title": "ikonwu"
  }
}